{
  "keyword": {
    "mapping": {
      "type": "keyword"
    }
  },
  "bool": {
    "mapping": {
      "type": "boolean"
    }
  },
  "long": {
    "mapping": {
      "type": "long"
    }
  },
  "keyword_uppercase": {
    "mapping": {
      "type": "keyword",
      "normalizer": "keyword_uppercase"
    }
  },
  "keyword_lowercase": {
    "mapping": {
      "type": "keyword",
      "normalizer": "keyword_lowercase"
    }
  },
  "keyword_trimmed": {
    "mapping": {
      "type": "keyword",
      "normalizer": "keyword_trimmed"
    }
  },
  "whitespace": {
    "mapping": {
      "type": "text",
      "analyzer": "whitespace"
    }
  },
  "date": {
    "mapping": {
      "type": "date",
      "format": "epoch_millis||strict_date_optional_time_nanos||strict_date_optional_time"
    }
  },
  "source": {
    "mapping": {
      "type": "keyword",
      "index": false
    }
  },
  "standard": {
    "mapping": {
      "type": "text",
      "analyzer": "source_analyzer"
    }
  },
  "multilang": {
    "mapping": {
      "properties": {
        "ara": {
          "type": "text",
          "analyzer": "arabic"
        },
        "ger": {
          "type": "text",
          "analyzer": "german"
        },
        "eng": {
          "type": "text",
          "analyzer": "english"
        },
        "spa": {
          "type": "text",
          "analyzer": "spanish"
        },
        "fre": {
          "type": "text",
          "analyzer": "french"
        },
        "heb": {
          "type": "text",
          "analyzer": "icu_analyzer"
        },
        "ita": {
          "type": "text",
          "analyzer": "italian"
        },
        "jpn": {
          "type": "text",
          "analyzer": "kuromoji"
        },
        "kor": {
          "type": "text",
          "analyzer": "seunjeon_analyzer"
        },
        "rus": {
          "type": "text",
          "analyzer": "russian"
        },
        "swe": {
          "type": "text",
          "analyzer": "swedish"
        },
        "chi": {
          "type": "text",
          "analyzer": "smartcn"
        },
        "src": {
          "type": "text",
          "analyzer": "source_analyzer"
        }
      }
    }
  }
}
