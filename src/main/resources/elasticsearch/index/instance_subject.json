{
  "index": {
    "number_of_shards": 4,
    "number_of_replicas": 2,
    "refresh_interval": "1s",
    "codec": "best_compression",
    "mapping.total_fields.limit": 1000,
    "sort.field": "value",
    "sort.order": "asc"
  },
  "analysis": {
    "normalizer": {
      "keyword_lowercase": {
        "filter": [ "lowercase" ],
        "type": "custom"
      }
    },
    "tokenizers": { }
  }
}
