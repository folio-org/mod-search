{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "urlPath": "/user-tenants",
        "queryParameters" : {
          "tenantId" : {
            "matches" : "test_tenant"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "GET",
        "urlPath": "/user-tenants",
        "queryParameters" : {
          "tenantId" : {
            "or": [
              { "matches" : "consortium" },
              { "matches" : "member_tenant" },
              { "matches" : "member2_tenant" }
            ]
          }
        }
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "userTenants": [
            {
              "centralTenantId": "consortium"
            }
          ]
        },
